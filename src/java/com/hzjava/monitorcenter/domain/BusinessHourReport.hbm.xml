<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="business_hour_report" name="com.hzjava.monitorcenter.domain.BusinessHourReport">
    <id access="field" name="id" type="java.lang.Long" column="Id">
      <generator class="identity"/>
    </id>
    <property name="businessName" column="business_name" access="field" type="java.lang.String"/>
    <property name="reportDate" column="report_date" access="field" type="java.util.Date"/>
    <property name="reportHour" column="report_hour" access="field" type="java.lang.Integer"/>
    <property name="extDataFlow" column="ext_dataflow" access="field" type="java.lang.Double"/>
    <property name="intDataFlow" column="int_dataflow" access="field" type="java.lang.Double"/>
    <property name="extRecordCount" column="ext_record_count" access="field" type="java.lang.Long"/>
    <property name="intRecordCount" column="int_record_count" access="field" type="java.lang.Long"/>
    <property name="alertCount" column="alert_count" access="field" type="java.lang.Long"/>
    <property name="state" column="state" access="field" type="int"/>
  </class>
</hibernate-mapping>
