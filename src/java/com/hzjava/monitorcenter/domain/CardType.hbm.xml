<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="cardtype" name="com.hzjava.monitorcenter.domain.CardType">
    <id access="field" name="id" type="java.lang.Long" column="Id">
      <generator class="identity"/>
    </id>
    <property name="name" column="type_name" access="field" type="java.lang.String"/>
    <property name="code" column="type_id" access="field" type="java.lang.String"/>
  </class>
</hibernate-mapping>
