<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="conf" name="com.hzjava.monitorcenter.domain.Conf">
    <id access="field" name="id" type="long" column="Id">
      <generator class="identity"/>
    </id>
    <property name="code" column="code" access="field" type="string"/>
    <property name="content" column="content" access="field" type="text"/>
  </class>
</hibernate-mapping>
