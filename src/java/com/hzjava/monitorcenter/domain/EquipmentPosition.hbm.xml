<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hzjava.monitorcenter.domain.EquipmentPosition" table="equipmentposition">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="equipmentid" type="int">
            <column name="equipmentid" />
        </property>
        <property name="ex" type="java.lang.String">
            <column name="ex" length="50" />
        </property>
        <property name="ey" type="java.lang.String">
            <column name="ey" length="50" />
        </property>


    </class>
</hibernate-mapping>