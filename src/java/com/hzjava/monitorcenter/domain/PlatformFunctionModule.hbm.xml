<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="platform_function_module" name="com.hzjava.monitorcenter.domain.PlatformFunctionModule">
    <id access="field" name="id" type="java.lang.Long" column="Id">
      <generator class="identity"/>
    </id>
    <property name="code" column="code" access="field" type="java.lang.String"/>
    <property name="codeDesc" column="code_desc" access="field" type="java.lang.String"/>
  </class>
</hibernate-mapping>
