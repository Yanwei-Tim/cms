<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="sbpz" name="com.hzjava.monitorcenter.domain.Sbpz">
    <id access="field" name="id" type="java.lang.Long" column="id">
      <generator class="identity"/>
    </id>
    <property name="level" column="level" access="field" type="java.lang.String"/>
    <property name="ip" column="ip" access="field" type="java.lang.String"/>
    <property name="port" column="port" access="field" type="java.lang.String"/>
    <property name="username" column="username" access="field" type="java.lang.String"/>
    <property name="password" column="password" access="field" type="java.lang.String"/>
    <property name="hour" column="hour" access="field" type="int"/>
    <property name="minute" column="minute" access="field" type="int"/>
    <property name="second" column="second" access="field" type="int"/>
    <property name="createdTime" column="created_time" access="field" type="java.util.Date"/>
    <property name="modifiedTime" column="modified_time" access="field" type="java.util.Date"/>
  </class>
</hibernate-mapping>
