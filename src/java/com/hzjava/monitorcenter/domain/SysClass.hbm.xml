<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="sys_class" name="com.hzjava.monitorcenter.domain.SysClass">
    <id access="field" name="id" type="string" column="system_class_code" length="5">
      <generator class="increment"/>
    </id>
    <property name="sysClassName" length="60" column="system_class_name" access="field" type="string"/>
  </class>
</hibernate-mapping>
