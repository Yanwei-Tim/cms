<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="sysabnormalinf" name="com.hzjava.monitorcenter.domain.SysAbnormalInf">
    <id access="field" name="id" type="java.lang.Long" column="Id">
      <generator class="native"/>
    </id>
    <property name="abnormalTypeCode" column="abnormalTypeCode" access="field" type="java.lang.String"/>
    <property name="connectObjectType" column="connectObjectCode" access="field" type="java.lang.String"/>
    <property name="exceptionDesc" column="exceptionDesc" access="field" type="java.lang.String"/>
    <property name="exceptionIf" column="exceptionIf" access="field" type="java.lang.String"/>
    <property name="action" column="action" access="field" type="java.lang.String"/>
    <property name="status" column="status" access="field" type="java.lang.String"/>
  </class>
</hibernate-mapping>
