<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="sysruntime" name="com.hzjava.monitorcenter.domain.Sysruntime">
    <id access="field" name="id" type="java.lang.Long" column="Id">
      <generator class="identity"/>
    </id>
    <property name="idSystem" column="Idsystem" access="field" type="java.lang.String"/>
    <property name="runStateCode" column="Runstatecode" access="field" type="java.lang.String"/>
    <property name="desc" column="Desc" access="field" type="java.lang.String"/>
    <property name="startTime" column="Starttime" access="field" type="java.util.Date"/>
    <property name="endTime" column="Endtime" access="field" type="java.util.Date"/>
    <property name="writeTime" column="writetime" access="field" type="java.util.Date"/>
  </class>
</hibernate-mapping>
