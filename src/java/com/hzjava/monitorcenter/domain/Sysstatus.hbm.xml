<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="sysstatus" name="com.hzjava.monitorcenter.domain.Sysstatus">
    <id access="field" name="id" type="java.lang.Long" column="Id">
      <generator class="identity"/>
    </id>
    <property name="idSystem" column="Idsystem" access="field" type="java.lang.String"/>
    <property name="access" column="Access" access="field" type="java.lang.Long"/>
    <property name="terminalNum" column="Terminalnum" access="field" type="java.lang.Long"/>
    <property name="inFlux" column="Influx" access="field" type="java.lang.Long"/>
    <property name="outFlux" column="Outflux" access="field" type="java.lang.Long"/>
    <property name="accessSum" column="Accesssum" access="field" type="java.lang.Long"/>
    <property name="bizSum" column="Bizsum" access="field" type="java.lang.Long"/>
    <property name="startTime" column="Starttime" access="field" type="java.util.Date"/>
    <property name="endTime" column="Endtime" access="field" type="java.util.Date"/>
    <property name="writeTime" column="writetime" access="field" type="java.util.Date"/>
  </class>
</hibernate-mapping>
