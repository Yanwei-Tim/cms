<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="terminal_address" name="com.hzjava.monitorcenter.domain.TerminalAddress">
    <id access="field" name="id" type="java.lang.Long" column="id">
      <generator class="assigned"/>
    </id>
    <property name="ip" column="ip" access="field" type="java.lang.String"/>
    <property name="port" column="port" access="field" type="java.lang.Integer"/>
  </class>
</hibernate-mapping>
