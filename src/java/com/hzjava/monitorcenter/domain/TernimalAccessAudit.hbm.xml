<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="ternimal_access_audit" name="com.hzjava.monitorcenter.domain.TernimalAccessAudit">
    <id access="field" name="id" type="java.lang.Long" column="Id">
      <generator class="identity"/>
    </id>
    <property name="cardType" column="card_type" access="field" type="java.lang.String"/>
    <property name="policeId" column="police_id" access="field" type="java.lang.String"/>
    <property name="policeName" column="police_name" access="field" type="java.lang.String"/>
    <property name="accessTime" column="access_time" access="field" type="java.util.Date"/>
    <property name="messageLevel" column="message_level" access="field" type="java.lang.String"/>
    <property name="desc" column="desc" access="field" type="java.lang.String"/>
    <property name="userId" column="userId" access="field" type="java.lang.String"/>
    <property name="userDepart" column="userDepart" access="field" type="java.lang.String"/>
    <property name="userZone" column="userZone" access="field" type="java.lang.String"/>
    <property name="flux" column="flux" access="field" type="java.lang.Integer"/>
    <property name="busName" column="busName" access="field" type="java.lang.String"/>
    <property name="count" column="count" access="field" type="java.lang.Integer"/>
  </class>
</hibernate-mapping>
